{
  "name": "applications-logger",
  "version": "1.0.0",
  "description": "Applications Logger",
  "main": "index.jsx",
  "private": true,
  "engines": {
    "node": "12.22.5"
  },
  "dependencies": {
    "@date-io/date-fns": "^2.14.0",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@loadable/component": "^5.15.2",
    "@mui/icons-material": "^5.8.3",
    "@mui/lab": "^5.0.0-alpha.85",
    "@mui/material": "^5.8.3",
    "axios": "^0.27.2",
    "date-fns": "^2.28.0",
    "material-design-icons": "^3.0.1",
    "moment": "^2.29.3",
    "node-sass": "^7.0.1",
    "npm": "^8.12.1",
    "prop-types": "^15.8.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1",
    "redux": "^4.2.0",
    "redux-saga": "^1.1.3",
    "sass": "^1.52.3",
    "sass-loader": "^13.0.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:import/recommended",
      "plugin:jsx-a11y/recommended"
    ],
    "plugins": [
      "react",
      "react-hooks",
      "flowtype",
      "import",
      "jsx-a11y",
      "testing-library"
    ],
    "settings": {
      "import/resolver": {
        "node": {
          "extensions": [
            ".js",
            ".jsx",
            ".ts",
            ".tsx"
          ]
        }
      }
    },
    "rules": {
      "no-mixed-operators": [
        "error",
        {
          "allowSamePrecedence": true
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/abdullah-yassin/applications-logger.git"
  },
  "author": "Abdullah Yassin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/abdullah-yassin/applications-logger.git/issues"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.2"
  }
}
